datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model Channel {
  id          Int      @id @default(autoincrement())
  name        String
  youtubeId   String   @unique
  subscribers Int
  views       Int
  videos      Int
  tracking    Boolean  @default(false)
}
